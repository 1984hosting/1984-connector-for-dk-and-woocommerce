{
    "name": "1984/woocoo",
    "type": "wordpress-plugin",
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.0",
        "slevomat/coding-standard": "^8.14",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpunit/phpunit": "^10.0",
        "psy/psysh": "^0.12.0",
        "yoast/phpunit-polyfills": "^2.0",
        "aldavigdis/wp-tests-strapon": "dev-main",
        "woocommerce/woocommerce": "8.6.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/aldavigdis/wp-tests-strapon.git"
        },
        {
            "type": "package",
            "package": {
                "name": "woocommerce/woocommerce",
                "version": "8.6.1",
                "dist": {
                    "url": "https://github.com/woocommerce/woocommerce/releases/download/8.6.1/woocommerce.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": false
        }
    },
    "autoload": {
        "psr-4": {
            "NineteenEightyFour\\NinteenEightyWoo\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "opis/json-schema": "^2.0@dev"
    }
}
